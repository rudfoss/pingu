{
	"editor.rulers": [120],
	"barrelr.lineEnding": "LF",
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": true
	},
	"[typescript]": {
		"editor.formatOnSave": true,
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[typescriptreact]": {
		"editor.formatOnSave": true,
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"search.exclude": {
		"**/node_modules": true,
		"**/.git": true,
		"**/yarn.lock": true,
		"**/.yarn": true,
		"**/.cache": true,
		"**/dist": true
	}
}